{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "wla",
	"scopeName": "source.wla",
	"patterns": [
		{
			"begin": "\\.include",
			"end": "\\s",
			"name": "keyword.control"
		},
		{
			"begin": "\\\"",
			"end": "\\\"",
			"name": "string"
		},
		{
			"begin": ";",
			"end": "$",
			"name": "comment.line"
		},{
			"begin": "^\\*",
			"end": "$",
			"name": "comment.line"
		},
		{
			"begin": "/\\*",
			"end": "\\*",
			"name": "comment.block"
		}


	]

}
		"comment": {
			"patterns": [
				{
					"begin": ";",
					"end": "$",
					"name": "comment.line"
				},
				{
					"begin": "^\\*",
					"end": "$",
					"name": "comment.line"
				},
				{
					"begin": "/\\*",
					"end": "\\*",
					"name": "comment.block"
				}
			]
		},
		"directive": {
			"patterns": [
